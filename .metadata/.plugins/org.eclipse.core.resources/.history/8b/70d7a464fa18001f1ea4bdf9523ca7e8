package in.microsoft.pojos;

public class Cart 
{
	private int id;
	private String name;
	private int quantity;
	
	public Cart() 
	{
		super();
		// TODO Auto-generated constructor stub
	}

	public Cart(int id, String name, int quantity) 
	{
		super();
		this.id = id;
		this.name = name;
		this.quantity = quantity;
	}
	
	public int getId()
	{
		return id;
	}
	
	public int setId()
	{
		return id;
	}

	public String getName() 
	{
		return name;
	}

	public void setName(String name) 
	{
		this.name = name;
	}

	public int getQuantity() 
	{
		return quantity;
	}

	public void setQuantity(int quantity) 
	{
		this.quantity = quantity;
	}

	@Override
	public String toString() 
	{
		return "Cart [name=" + name + ", quantity=" + quantity + "]";
	}

}
